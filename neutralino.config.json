{
	"applicationId": "js.neutralino.zero",
	"version": "1.0.0",
	"defaultMode": "window",
	"documentRoot": "/vite-src/dist/",
	"url": "/",
	"port": 3000,
	"enableServer": true,
	"enableNativeAPI": true,
	"nativeAllowList": ["app.*", "filesystem.readDirectory"],
	"modes": {
		"window": {
			"title": "myapp",
			"width": 800,
			"height": 500,
			"minWidth": 400,
			"minHeight": 200,
			"icon": "/vite-src/public/neutralino.png",
			"enableInspector": true
		}
	},
	"cli": {
		"binaryName": "myapp",
		"resourcesPath": "/vite-src/dist/",
		"extensionsPath": "/extensions/",
		"binaryVersion": "5.4.0",
		"clientVersion": "5.4.0",
		"frontendLibrary": {
			"patchFile": "/vite-src/index.html",
			"devUrl": "http://localhost:5173",
			"projectPath": "/vite-src/",
			"initCommand": "npm install",
			"devCommand": "npm run dev",
			"buildCommand": "npm run build"
		}
	}
}
